
{
  "executionRoleArn": "arn:aws:iam::720230680132:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "cbdb-api-container",
          "image": "<IMAGE1>",
          "essential": true,
          "portMappings": [
              {
                  "hostPort": 80,
                  "protocol": "tcp",
                  "containerPort": 80
              },
              {
                "hostPort": 5000,
                "protocol": "tcp",
                "containerPort": 5000
            }
          ]
      }
  ],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "cbdb-family"
}